#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_webpack-cli@3.1.2@webpack-cli/bin/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../_webpack-cli@3.1.2@webpack-cli/bin/cli.js" "$@"
  ret=$?
fi
exit $ret
